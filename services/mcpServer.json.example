{
  "mcpServers": {
    "sherlock-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/sherlock-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    },
    "holehe-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/holehe-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    },
    "maigret-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/maigret-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    },
    "blackbird-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/blackbird-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    },
    "spiderfoot-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/spiderfoot-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "SHODAN_API_KEY": "your_shodan_api_key_here",
        "VIRUSTOTAL_API_KEY": "your_virustotal_api_key_here",
        "HAVEIBEENPWNED_API_KEY": "your_hibp_api_key_here",
        "ABUSEIPDB_API_KEY": "your_abuseipdb_api_key_here",
        "CENSYS_API_ID": "your_censys_api_id_here",
        "CENSYS_API_SECRET": "your_censys_api_secret_here",
        "GREYNOISE_API_KEY": "your_greynoise_api_key_here",
        "INTELX_API_KEY": "your_intelx_api_key_here",
        "PASSIVETOTAL_API_USERNAME": "your_passivetotal_username_here",
        "PASSIVETOTAL_API_KEY": "your_passivetotal_api_key_here",
        "SECURITYTRAILS_API_KEY": "your_securitytrails_api_key_here"
      }
    },
    "ghunt-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/ghunt-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "GOOGLE_API_KEY": "your_google_api_key_here",
        "GOOGLE_CX": "your_google_custom_search_engine_id_here",
        "GOOGLE_CLIENT_ID": "your_google_client_id_here",
        "GOOGLE_CLIENT_SECRET": "your_google_client_secret_here"
      }
    },
    "theharvester-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "hackerdogs/theharvester-mcp-server:latest"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "HUNTER_API_KEY": "your_hunter_io_api_key_here",
        "BING_API_KEY": "your_bing_api_key_here",
        "SHODAN_API_KEY": "your_shodan_api_key_here",
        "SECURITYTRAILS_API_KEY": "your_securitytrails_api_key_here"
      }
    }
  }
}
